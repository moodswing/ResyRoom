@using System.Web.Script.Serialization
@model ResyRoom.ViewModels.RegisterStudio.IRegisterStudioViewModel
@{
    ViewBag.Title = "Registrar nuevo usuario";
}

@Scripts.Render("~/bundles/knockoutjs-with-mapping")
@Scripts.Render("~/bundles/history")
@Scripts.Render("~/bundles/RegisterStudio")
<script type="text/javascript">
    var koViewModel;
    var jsModel;
    
    $(document).ready(function () {
        jsModel = @Html.Raw(new JavaScriptSerializer().Serialize(Model));
        var viewKoViewModel = ko.mapping.fromJS(jsModel);
        
        koViewModel = RegisterStudioConfiguration(viewKoViewModel).Init();
    });
</script>
<div class="content">
    <div class="form">
        <div class="register-studio-form">
        </div>
    </div>
</div>
